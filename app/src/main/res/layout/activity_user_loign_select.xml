<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserLoignSelect">

    <Button
        android:id="@+id/btn_adminLogin"
        android:layout_width="240dp"
        android:layout_height="71dp"
        android:layout_marginTop="40dp"
        android:onClick="NavigatetoAdminLogin"
        android:text="@string/btn_adminLogin"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:backgroundTint="#FF9A56"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_userLogin" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="759dp"
        android:layout_height="285dp"
        android:layout_marginTop="-1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/foodieloginimage" />

    <Button
        android:id="@+id/btn_userLogin"
        android:layout_width="240dp"
        android:layout_height="68dp"
        android:layout_marginTop="40dp"
        android:onClick="NavigatetoUserLogin"
        android:text="@string/btn_userLogin"
        android:textAllCaps="false"
        android:textSize="24sp"
        app:backgroundTint="#FF9A56"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

</androidx.constraintlayout.widget.ConstraintLayout>